{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "IncludeScopes": false,
        "TimestampFormat": "MM-dd HH:mm:ss ",
        "UseUtcTimestamp": false
      }
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5000"
      }
    }
  },
  "GameServer": "http://127.0.0.1:5000/",
  "AllowedHosts": "*",
  "MainDB": "Main", 
  "DBS": [
    {
      "ConnId": "Main",
      "DBType": 2,
      "Enabled": true,
      "Connection": "game.db"
    },
    {
      "ConnId": "Log",
      "DBType": 2,
      "Enabled": true,
      "Connection": "game.db"
    }
  ]
}
